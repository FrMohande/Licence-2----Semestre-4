<?php
	function comparaison($a,$b) {
		$c ;
		if ( $a == $b) {
			$c = 0 ;
 		} else {
			if ( $a > $b ) {
				$c = 1 ; 
			} else {	
				$c = -1 ;
			}
		
		}
		return $c ;
	}
	$toto = comparaison(4,3) ;
	echo $toto ;
?>
